{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Dobri Next -A03- Mirage",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "liveServer.settings.donotShowInfoMsg": true,
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "json.schemas": [],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "editor.fontLigatures": null,
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "editor.largeFileOptimizations": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "tabnine.experimentalAutoImports": true,
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.activeIconPack": "react_redux",
  "material-icon-theme.folders.color": "#90a4ae",
  "material-icon-theme.hidesExplorerArrows": false,
  "material-icon-theme.saturation": 1,
  "material-icon-theme.opacity": null,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.confirmSync": false,
  "diffEditor.wordWrap": "on",
  "editor.wordWrap": "on",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "intelephense.format.braces": "k&r",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true // this is the important one for you!!
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.enablePreview": false,
  "prettier.trailingComma": "all",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": true,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground2": "#9F51B6",
    "editorBracketHighlight.foreground3": "#F7C244",
    "editorBracketHighlight.foreground4": "#F07850",
    "editorBracketHighlight.foreground5": "#9CDD29",
    "editorBracketHighlight.foreground6": "#C497D4"
  },
  "dart.debugExternalPackageLibraries": true,
  "dart.debugSdkLibraries": true,
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "files.eol": "\n",
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "typescript.format.enable": false
}